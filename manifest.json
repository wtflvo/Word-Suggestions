{
	// Required
	"manifest_version": 3,
	"name": "Word Suggestions",
	"version": "1.0",
	"author": "Viacheslav Lutskov",
	// "background": {
	// 	"service_worker": "background.js"
	// },
	"permissions": ["activeTab"],
	"action": {
		"default_icon": {
			"19": "icon16px.png",
			"38": "icon48px.png"
		}
	},
	//   "19": "images/icon19.png",
	//	  "38": "images/icon38.png"
	// 	},
	// 	"default_popup": "popup.html"
	//   },
	"icons": {
		"16": "icon16px.png",
		"48": "icon48px.png",
		"128": "icon128px.png"
	},
	// "default_locales": "en",
	"description": "Suggestions for the input text elements and contenteditable elements",
	"content_scripts": [
		{ "matches": ["<all_urls>"], "js": ["index.js"], "css": ["index.css"] }
	]
}
// "default_popup": "popup.html"

